{
  "name": "pangea",
  "version": "0.1.0",
  "dependencies": {
    "@expo/vector-icons": "^5.1.0",
    "expo": "^19.0.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-alpha.12",
    "react-dom": "^16.0.0-alpha.12",
    "react-native": "0.46.4",
    "react-native-drawer": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.11",
    "react-test-renderer": "^16.0.0-alpha.12",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "android": "react-native-scripts android",
    "build": "cross-env NODE_ENV='production' react-scripts build",
    "eject": "react-scripts eject",
    "eject-native": "react-native-scripts eject",
    "ios": "react-native-scripts ios",
    "lint": "node node_modules/eslint/bin/eslint.js --ext .js src/ --cache",
    "start": "cross-env NODE_ENV='development' react-scripts start",
    "start-native": "react-native-scripts start",
    "test": "cross-env NODE_ENV='testing' node node_modules/jest/bin/jest.js",
    "test-watch": "cross-env NODE_ENV='testing' node node_modules/jest/bin/jest.js --no-cache --watch",
    "test-coverage": "cross-env NODE_ENV='testing' node node_modules/jest/bin/jest.js --coverage"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "cross-env": "^5.1.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "jest": "^20.0.4",
    "jest-expo": "^20.0.0",
    "react-native-mock": "^0.3.1",
    "react-native-scripts": "^1.1.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "sinon": "^4.0.1"
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "jest": {
    "bail": true,
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "App.js"
    ],
    "setupFiles": [
      "<rootDir>/src/_tests_/setup-before-after.js"
    ],
    "testRegex": "src/_tests_/.*\\.test\\.js",
    "transformIgnorePatterns": [
      "/!node_modules\\/\\@expo"
    ],
    "preset": "jest-expo",
    "unmockedModulePathPatterns": [
      "node_modules",
      "@expo"
    ],
    "verbose": true
  }
}
